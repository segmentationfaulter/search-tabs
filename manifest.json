{
  "manifest_version": 2,
  "name": "search-tabs",
  "description": "Search open tabs using vim keybindings",
  "version": "0.1",
  "browser_action": {
    "default_title": "Search Open Tabs",
    "default_popup": "./src/index.html"
  },
  "permissions": ["tabs"],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    }
  }
}
